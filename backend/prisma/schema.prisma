generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id() @default(autoincrement())
  name String
  username String @unique
  password String
  level String @default("user")
  status String @default("use")
}

model Room{
  id Int @id() @default(autoincrement())
  name String
  price Int
  status String @default("use")

}